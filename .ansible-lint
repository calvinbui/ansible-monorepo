skip_list:
  - empty-string-compare
  - git-latest
  - jinja[invalid]
  - jinja[spacing]
  - name[play]
  - name[template]
  - no-handler
  - no-tabs
  - template-instead-of-copy
  - yaml[line-length]

exclude_paths:
  - inventory.yml

enable_list:
  - empty-string-compare
  - no-log-password
  - no-same-owner
  - yaml
